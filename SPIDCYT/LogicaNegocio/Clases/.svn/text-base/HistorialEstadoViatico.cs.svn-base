using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

/// <summary>
/// Representa los distinos estados por los que pasó un viático.
/// Permite a un Investigador hacer un seguimiento del estado de los mismos.
/// </summary>
    public class HistorialEstadoViatico
    {
        private DateTime fechaDesde;
        private EstadoViatico estado;

        public EstadoViatico ESTADO
        {
            get { return estado; }
            set { estado = value; }
        }

        public DateTime FECHADESDE
        {
            get { return fechaDesde; }
            set { fechaDesde = value; }
        }
    }

